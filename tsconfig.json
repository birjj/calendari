{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "/workspaces/calendari",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@server/*": [
        "src/server/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}