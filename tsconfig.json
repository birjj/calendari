{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "/workspaces/calendari",
    "paths": {
      "@components/*": ["src/components/*"],
      "@server/*": ["src/server/*"],
      "@utils/*": ["src/utils/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"]
    },
    "types": ["unplugin-icons/types/vue"],
    "jsx": "preserve",
    "jsxImportSource": "vue"
  }
}
